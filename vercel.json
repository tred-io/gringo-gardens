{
  "buildCommand": "cd client && vite build --outDir ../public --emptyOutDir",
  "outputDirectory": "public",
  "cleanUrls": false,
  "trailingSlash": false,
  "functions": {
    "api/**/*.js": {
      "runtime": "@vercel/node@3.2.0"
    }
  },
  "rewrites": [
    {
      "source": "/((?!api).*)",
      "destination": "/index.html"
    }
  ],
  "version": 2
}