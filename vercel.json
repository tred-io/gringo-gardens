{
  "buildCommand": "cd client && vite build --outDir ../public --emptyOutDir",
  "outputDirectory": "public",
  "cleanUrls": false,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "version": 2
}